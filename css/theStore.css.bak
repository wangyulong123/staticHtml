	text-align: center;
	text-decoration: none;
}

.nav a:hover {
	background-color: #fff;
	color: #FF4040;
}

.nav ul a:hover {
	color: #C12929;
}

.nav>li {
	border-left: 1px solid #F4F4F4;
	border-right: 1px solid #F4F4F4;
}

.nav>li:hover {
	border-color: #E5E5E5;
	background-color: #fff;
}


/*让li下所有的ul先隐藏*/

li>ul {
	display: none;
}


/*让鼠标悬浮的li下的ul显示出来*/

.nav>li:hover>ul {
